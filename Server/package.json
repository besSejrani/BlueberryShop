{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run copy && ts-node index.ts",
    "build": "tsc",
    "copy": "cp ./src/Graphql/schema/schema.gql ./dist/Graphql/schema ",
    "mesh": "graphql-mesh serve",
    "server": "tsnd --respawn -r tsconfig-paths/register index.ts",
    "client": "npm run dev --prefix ../Client",
    "client:cypress": "cd ../Client && yarn run cypress:open",
    "documentation": "cd ../Documentation && mkdocs serve",
    "dev": "concurrently \"npm run server\" \"npm run client\" \" npm run documentation\" \" npm run client:cypress\""
  },
