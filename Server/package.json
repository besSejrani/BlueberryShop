{
  "name": "serverd",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run copy && ts-node index.ts",
    "build": "tsc",
    "copy": "cp ./src/Graphql/schema/schema.gql ./dist/Graphql/schema ",
    "server": "ts-node-dev --respawn index.ts",
    "client": "npm run dev --prefix ../Client",
    "documentation": "cd ../Documentation && mkdocs serve",
    "dev": "concurrently \"npm run server\" \"npm run client\" \" npm run documentation\""
  },
  "dependencies": {
    "@typegoose/typegoose": "^7.4.8",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/graphql-upload": "^8.0.4",
    "@types/ioredis": "^4.22.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.31",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-sendgrid": "^1.0.0",
    "@types/passport-github2": "^1.2.4",
    "@types/passport-google-oauth20": "^2.0.6",
    "@types/uuid": "^8.3.0",
    "apollo-server-express": "^2.21.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-upload": "^11.0.0",
    "helmet": "^4.4.1",
    "ioredis": "^4.23.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "5.10.18",
    "nodemailer": "^6.5.0",
    "nodemailer-sendgrid": "^1.0.3",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "type-graphql": "^1.1.1",
    "typescript": "^4.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "prettier": "^2.2.1"
  }
